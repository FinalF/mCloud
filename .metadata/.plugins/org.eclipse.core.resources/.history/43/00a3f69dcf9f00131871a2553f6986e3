package AbClasses;

import Interfaces.InfoItem;

abstract class InfoItemSlot implements InfoItem{
	//	private String type=null;
	//	private String size=null;
	//	private String count=null;
	private String[] item = new String[3];
	
	InfoItemSlot(){
		item[0]=null;
		item[1]=null;
		item[2]=null;
	}
	
	
	InfoItemSlot(String type, String count, String size){
		item[0]=null;
		item[1]=null;
		item[2]=null;
	}
	
	
	 public String returnType(){
		return item[0];
	 }

	 public int returnCount(){
		 return Integer.parseInt(item[2]);
	 }

	 public int returnSize(){
		 return Integer.parseInt(item[1]);
	 }

}
